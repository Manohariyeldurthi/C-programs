#include <stdio.h>

int main()
{
    int n,sum=0,pro=1,r,result;
    printf("enter any number:");
    scanf("%d",&n);
    while(n>0)
    {
        r=n%10;
        pro=pro*r;
        sum=sum+r;
        n=n/10;
        result=pro-sum;
    }
    printf("product of digits:%d",pro);
     printf("sum of digits:%d\n",sum);
    printf("result:%d",result);
    return 0;
    }
